{
  "name": "favgen",
  "version": "0.0.0-alpha1",
  "description": "CLI tool to generate a set of favicons from a single SVG input.",
  "main": "index.js",
  "keywords": [
    "favicon"
  ],
  "author": "islambeg",
  "license": "Unlicense",
  "bin": "bin/index.js",
  "scripts": {
    "build:dev": "tsc --watch --project tsconfig.json",
    "prepare": "husky install && husky add .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore --cache",
    "prettify": "prettier --write --ignore-path .gitignore --plugin-search-dir=."
  },
  "dependencies": {
    "commander": "^9.4.0",
    "sharp": "^0.30.7",
    "to-ico": "^1.1.5"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/sharp": "^0.30.4",
    "@types/to-ico": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
