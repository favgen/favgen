{
  "name": "favgen",
  "version": "0.2.7",
  "description": "CLI tool to generate a set of favicons from a single input file.",
  "keywords": [
    "favicon"
  ],
  "author": "islambeg",
  "license": "Unlicense",
  "homepage": "https://github.com/favgen/favgen",
  "repository": "https://github.com/favgen/favgen.git",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "bin": "bin/index.js",
  "files": [
    "lib/",
    "bin/"
  ],
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:dev": "tsc --watch --project tsconfig.json",
    "test": "npm run build && vitest run",
    "test:watch": "npm run build && vitest",
    "prepublishOnly": "npm run build",
    "huskify": "husky install && husky add .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore --cache",
    "prettify": "prettier --write --ignore-path .gitignore --plugin-search-dir=."
  },
  "dependencies": {
    "commander": "^9.4.0",
    "is-svg": "^4.3.2",
    "sharp": "^0.30.7",
    "svgo": "^2.8.0",
    "to-ico": "^1.1.5"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/sharp": "^0.30.4",
    "@types/svgo": "^2.6.3",
    "@types/to-ico": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vitest": "^3.2.4"
  }
}
